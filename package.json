{
	"name": "wedding-site",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently --kill-others -n server,studio 'npm:dev:server' 'npm:dev:studio'",
		"dev:server": "next dev",
		"dev:studio": "prisma studio",
		"build": "next build",
		"start": "next start",
		"export": "npm run build && next export",
		"db:test-query": "ts-node ./prisma/test-prisma-query.ts",
		"db:reset": "prisma migrate reset",
		"db:migrate": "prisma migrate dev",
		"prepare": "husky install"
	},
	"dependencies": {
		"@headlessui/react": "^1.4.3",
		"@heroicons/react": "^1.0.5",
		"@prisma/client": "^3.10.0",
		"@xstate/react": "^2.0.0",
		"airtable": "^0.11.2",
		"axios": "^0.26.0",
		"date-fns": "^2.28.0",
		"datebook": "^7.0.1",
		"next": "^12.1.1",
		"qs": "^6.10.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"xstate": "^4.27.0"
	},
	"devDependencies": {
		"@netlify/plugin-nextjs": "^4.2.7",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/typography": "^0.5.2",
		"@types/node": "16.11.19",
		"@types/react": "^17.0.20",
		"@xstate/inspect": "^0.6.0",
		"autoprefixer": "^10.4.2",
		"concurrently": "^7.0.0",
		"eslint": "^8.9.0",
		"eslint-config-next": "^12.1.0",
		"husky": "^7.0.2",
		"lint-staged": "^12.3.4",
		"postcss": "^8.4.6",
		"prettier": "^2.4.0",
		"prisma": "^3.10.0",
		"tailwindcss": "^3.0.23",
		"ts-node": "^10.5.0",
		"typescript": "^4.5.4"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "eslint",
		"*.{js,jsx,ts,tsx,css,md}": "prettier --write"
	},
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	}
}
